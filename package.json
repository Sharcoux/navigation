{
  "name": "react-unavigation",
  "version": "1.1.7",
  "description": "React Universal Navigation",
  "main": "index.js",
  "scripts": {
    "prepare": "tsc && babel index.tsx --extensions '.tsx' > index.js",
    "web": "babel app.web.tsx --extensions '.tsx' > app.web.js && parcel index.html",
    "open": "xdg-open http://localhost:1234",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Sharcoux/react-unavigation.git"
  },
  "keywords": [
    "react",
    "native",
    "navigation",
    "web"
  ],
  "author": "fbillioud",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sharcoux/react-unavigation/issues"
  },
  "homepage": "https://github.com/Sharcoux/react-unavigation#readme",
  "peerDependencies": {
    "react-native": ">=0.62.2",
    "react": ">=16.13.1",
    "styled-components": ">=5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/react": "^16.9.35",
    "@types/react-native": "^0.62.13",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-plugin-standard": "^4.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-native": "^0.61.5",
    "release-it": "^13.6.2",
    "typescript": "^3.9.5"
  },
  "files": [
    "index.js",
    "index.tsx",
    "index.d.ts"
  ],
  "alias": {
    "react-native": "react-native-web"
  },
  "types": "index.d.ts",
