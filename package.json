{
  "name": "@react-native-assets/navigation",
  "version": "2.1.2",
  "description": "React Universal Navigation",
  "main": "dist/index.js",
  "browser": "dist/index.web.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "parcel build src/index.tsx --dist-dir dist/index.js && parcel build src/index.web.tsx --dist-dir dist/index.web.js",
    "dev": "parcel example/index.html",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Sharcoux/react-unavigation.git"
  },
  "keywords": [
    "react",
    "native",
    "navigation",
    "web"
  ],
  "author": "fbillioud",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sharcoux/react-unavigation/issues"
  },
  "homepage": "https://github.com/Sharcoux/react-unavigation",
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-native": ">=0.63.2"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "^7",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "eslint-plugin-standard": "^4.1.0",
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.0",
    "react-native-web": "^0.19.0",
    "release-it": "^14.2.2",
    "rimraf": "^5.0.0",
    "typescript": "^5.6.2"
  },
  "files": [
    "dist",
    "src"
  ],
  "alias": {
    "react-native": "react-native-web"
  }
}
