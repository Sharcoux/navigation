{
  "name": "@react-native-assets/navigation",
  "version": "2.1.3",
  "description": "React Universal Navigation (React Native version)",
  "main": "dist/index.js",
  "browser": "dist/index.web.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && parcel build src/index.tsx --dist-dir dist/index.js --no-source-maps && parcel build src/index.web.tsx --dist-dir dist/index.web.js --no-source-maps",
    "clean": "rimraf dist",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Sharcoux/react-unavigation.git"
  },
  "keywords": [
    "react",
    "react-native",
    "navigation",
    "web"
  ],
  "author": "fbillioud",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-native": ">=0.63.2"
  },
  "devDependencies": {
    "@types/react-native": "^0.72.0",
    "eslint-plugin-react-native": "^3.10.0",
    "react-native": "^0.79.2",
    "react-native-web": "^0.20.0"
  },
  "alias": {
    "react-native": "react-native-web"
  },
  "publishConfig": {
    "access": "public"
  }
}
